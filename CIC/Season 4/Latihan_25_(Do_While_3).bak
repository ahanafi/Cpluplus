/*
	ALGORITMA mencetak_angka

   DEKLARASI
   	angka : integer
      n : intger

   DESKRIPSI
   	write('Masukkan banyak n : ')
      read(n)

      while(angka <= n) do
      	write(angka)
         angka++
      endwhile
*/

#include <stdio.h>
#include <conio.h>

main()
{
	int angka = 1, n;

   printf("Masukkan banyak n : ");
   	scanf("%d", &n);

   do{
   	printf("%d \n", angka);
      angka++;
   }while(angka <= n);
   getch();
}
